{
  "name": "avrillon.me",
  "version": "2.6.3",
  "main": "index.js",
  "private": true,
  "repository": "git@github.com:kavrillon/avrillon.me.git",
  "author": "Kevin Avrillon <kevin@avrillon.me>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "file-loader": "^3.0.1",
    "html-webpack-exclude-assets-plugin": "^0.0.7",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.5.0",
    "jest-environment-puppeteer": "^4.0.0",
    "jest-image-snapshot": "^2.8.1",
    "lighthouse": "^4.2.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "puppeteer": "^1.13.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-concentric-order": "^3.4.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.4",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^4.1.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "scripts": {
    "build": "yarn build:prod",
    "build:dev": "webpack",
    "build:prod": "NODE_ENV=production webpack",
    "deploy": "bash scripts/deploy.sh",
    "format": "prettier --write '**/*.{js,md,json,scss}' && stylelint 'src/**/*.scss' --fix",
    "lint": "npm-run-all -p lint:*",
    "lint:prettier": "yarn prettier -l 'src/**/*.{js,md,json,scss}'",
    "lint:scss": "stylelint 'src/**/*.scss'",
    "release": "bash scripts/release.sh",
    "start": "yarn start:dev",
    "start:dev": "webpack-dev-server --progress --colors",
    "start:prod": "NODE_ENV=production webpack-dev-server",
    "test": "yarn test:e2e",
    "test:e2e": "jest",
    "test:e2e:audit": "jest test/e2e/audit.spec.js --detectOpenHandles",
    "test:e2e:dom": "jest test/e2e/dom.spec.js",
    "test:e2e:snapshot": "jest test/e2e/snapshot.spec.js"
  },
  "dependencies": {
    "clipboard-copy": "^3.0.0"
  }
}
